{
  "name": "mdast-util-from-adf",
  "version": "2.1.1",
  "description": "Convert from Atlassian Document Format to Markdown Abstract Syntax Tree",
  "license": "MIT",
  "author": {
    "name": "bitcrowd GmbH.",
    "url": "https://bitcrowd.net",
    "email": "info@bitcrowd.net"
  },
  "repository": "github:bitcrowd/mdast-util-from-adf",
  "type": "module",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "del-cli dist && tsc --project tsconfig.dist.json",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --max-warnings 0 .",
    "prettier": "prettier --check '**/*.{ts,js,json,md}' --ignore-path .gitignore",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@atlaskit/adf-schema": "^24.0.0",
    "@types/mdast": "^3.0.0",
    "unist-builder": "^3.0.0"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitest/ui": "^1.0.0",
    "chance": "^1.1.8",
    "del-cli": "^4.0.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^1.0.0"
  }
}
